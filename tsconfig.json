{
  "compilerOptions": {
    "allowJs": true,
    "lib": [
      "es6",
      "dom",
      "es2017"
    ],
    "module": "es2020",
    "noImplicitAny": true,
    "strict": true,
    "strictNullChecks": false,
    "sourceMap": true,
    "target": "es2015",
    "moduleResolution": "node"
  },
  "exclude": [
    "node_modules",
    "webpack.*.js"
  ]
}



